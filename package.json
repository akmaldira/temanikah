{
  "name": "temanikah",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "yarn build && cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "migration:create": "typeorm migration:create -o ./migrations/migration",
    "migration:generate": "yarn build && cross-env NODE_ENV=production typeorm migration:generate -o ./migrations/migration -d ./dist/database/datasource.js",
    "migration:run": "cross-env NODE_ENV=production typeorm migration:run -d ./dist/database/datasource.js",
    "migration:revert": "cross-env NODE_ENV=production typeorm migration:revert -d ./dist/database/datasource.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.1",
    "midtrans-client": "^1.3.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "valibot": "^0.12.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.77",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "ts-config": "^20.10.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
